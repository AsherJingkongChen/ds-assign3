chmod +x *.sh;

[ -f ~/.zshrc ] && source ~/.zshrc;
[ -f ~/.bashrc ] && source ~/.bashrc;
[ -f ~/.tcshrc ] && source ~/.tcshrc;
[ -f ~/.profile ] && source ~/.profile;
[ -f ~/.bash_profile ] && source ~/.bash_profile;

CXX="g++";

CXXFLAGS=\
"-std=c++17 \
-O3 \
-w";

NAMES=(
  "insert/btree"
  "search/btree"
  "insert/skiplist"
  "search/skiplist"
  "insert/treap"
  "search/treap"
  "insert/hashtable"
  "search/hashtable"
  "insert/array_of_sorted_arrays"
  "search/array_of_sorted_arrays"
);

FROM=10; TO=20;
TIMEOUT_SECOND=3600;

RESULT_HEADER=\
"structure_name,\
operation_name,\
size_in_2_power_of,\
time_in_millisecond";

RESULT_NAME="output/test/summary/summary.csv";
